{
  "Jellyfin": {
    "ServerUrl": "http://localhost:8096",
    "ApiKey": "your-jellyfin-api-key-here"
  },
  "AniList": {
    "UserTokens": {
      "your-jellyfin-username": "your-anilist-access-token-here"
    }
  },
  "Webhook": {
    "Host": "localhost",
    "Port": 5000
  },
  "Sonarr": {
    "Enabled": false,
    "RefreshJellyfinOnImport": true,
    "ApiKey": "YOUR_SONARR_API_KEY_HERE"
  },
  "LibraryNames": ["Anime", "TV Shows"],
  "UserSettings": {
    "your-jellyfin-username": {
      "userAutoAdd": true,
      "userBulkUpdate": true
    }
  }
}
